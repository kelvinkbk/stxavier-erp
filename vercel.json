{
  "version": 2,
  "framework": null,
  "buildCommand": "npm run build:web",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "env": {
    "EXPO_PUBLIC_FIREBASE_API_KEY": "AIzaSyAXUmn2ZHX3s_ZqaxDwqGYxiDmKJUj-AY8",
    "EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN": "stxavier-erp.firebaseapp.com",
    "EXPO_PUBLIC_FIREBASE_PROJECT_ID": "stxavier-erp",
    "EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET": "stxavier-erp.appspot.com",
    "EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID": "588752936214",
    "EXPO_PUBLIC_FIREBASE_APP_ID": "1:588752936214:web:99389d56ca0f9395d40437",
    "EXPO_PUBLIC_ENVIRONMENT": "production",
    "EXPO_PUBLIC_DEBUG_MODE": "false"
  },
  "regions": ["sin1"],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    },
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
